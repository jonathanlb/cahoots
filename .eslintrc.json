{
    "extends": [ "eslint:recommended" ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-console": [ "warn" ],
      "no-trailing-spaces": [ "error" ],
      "quotes": [ "warn", "single" ]
    }
}
